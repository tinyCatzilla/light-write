{
    "compilerOptions": {
      "outDir": "./dist",
      "target": "es6",
      "module": "commonjs",
      "jsx": "react",
      "moduleResolution": "node",
      "sourceMap": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noUnusedLocals": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    },
    "include": [
      "./src/**/*"
    ]
}
  